{% extends 'base.html.twig' %}

{% block title %}Hello ProjetsController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">

    {% for projet in projets %}
        {{projet.nom}}
        {{projet.attachments.snapshot.0.image}}
    {% endfor %}
<div class="slideup">
    <img width="1920" height="4590" src="https://premiumaddons.b-cdn.net/wp-content/uploads/2019/09/cafe-wiz.jpg" class="attachment-full size-full" alt="Image Scroll Widget for Elementor" srcset="https://premiumaddons.b-cdn.net/wp-content/uploads/2019/09/cafe-wiz.jpg 1920w, https://premiumaddons.b-cdn.net/wp-content/uploads/2019/09/cafe-wiz-125x300.jpg 125w, https://premiumaddons.b-cdn.net/wp-content/uploads/2019/09/cafe-wiz-768x1836.jpg 768w, https://premiumaddons.b-cdn.net/wp-content/uploads/2019/09/cafe-wiz-428x1024.jpg 428w" sizes="(max-width: 1920px) 100vw, 1920px" style="transform: translateY(0px);">                    </div>
</div>
    
</div>
{% endblock %}
